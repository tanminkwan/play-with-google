{
    "manifest_version": 3,
    "name": "Antigravity Video uploader",
    "version": "1.0",
    "description": "Upload video metadata to n8n for YouTube registration.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}